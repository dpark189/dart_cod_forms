<h1>INDEX</h1>
<ul>
  <li>
    <%= link_to 'See All Accounts', :controller => "accounts", :completed => "all" %>
  </li>
  <li>
    <%= link_to 'See Only Completed Accounts', :controller => "accounts", :completed => "true" %>
  </li>
  <li>
    <%= link_to 'See Only Uncompleted Accounts', :controller => "accounts", :completed => "false" %>
  </li>
</ul>
<section id="table_section">
  <div class="table_div">
    <table>
      <tr>
        <% @accounts.first.attributes.keys.each do |key| %>
        <% next if @secondary_attr.include?(key) %>
        <th><%= key.titleize %></th>
        <% end %>
      </tr>
      <% @accounts.each do |account| %>
      <tr>
        <%= render "row_form.html.erb", account: account %>
      </tr>
      <% end %>
    </table>
  </div>
</section>
